  "scripts": {
    "buildData": "npm run buildIndex & npm run createReadme",
    "buildIndex": "ts-node --project ./scripts/tsconfig.json ./scripts/transformDataForWeb.ts build",
    "clean": "rm -rf ./dist",
    "createReadme": "ts-node --project ./scripts/tsconfig.json ./scripts/json2readme.ts > README.md",
    "lint": "tslint -p tsconfig.json -e '**/*.js' -t stylish",
    "lint:fix": "tslint -p tsconfig.json -e '**/*.js' -t stylish --fix",
    "prod": "npm run clean & npm run buildData & webpack --progress --config webpack.prod.js",
    "start": "npm run buildData & webpack-dev-server --config webpack.config.js",
    "test": "cross-env NODE_ENV=test jest --no-cache --coverage --config=config/test/jestconfig.json --maxWorkers=2",
    "test:dev": "cross-env NODE_ENV=test jest --watch  --config=config/test/jestconfig.json",
    "types": "tsc --noEmit"
  },
